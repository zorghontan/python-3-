import getpass

Name = input("Introduce nombre de usuario: ")
Password = getpass.getpass(prompt = "Introduce la contrase単a: ", stream =None)

while Name != "Camilo" or Name != "camilo" and Password != "123":
    if Name == "Camilo" or Name == "camilo" and Password == "123":
        print("\n\tDatos correctos")
        print("Ingresando al programa")
        break
    else:
        print("\nContrase単a o nombre de usuario incorrecto")
        print("\nReintentar contrase単a y nombre de usuario\n")
        Name = input("Introduce nombre de usuario: ")
        Password = getpass.getpass("\nIntroduce la contrase単a: ")
